//NESTED IF-ELSE....



#include<stdio.h>
int main()
{
    int age;
    printf("Enter your age: ");
    scanf("%d",&age);

    if(age<2){
        printf("Infant\n");
    }else if(age>=2 && age<10){
        printf("Child");
    }else if(age>=10 && age<20){
        printf("Teenage");
    }else if(age>=20 && age<30){
        printf("Adult");
    }else if(age>30){
        printf("Old");
    }else{
        printf("Error(404)");
    }
    return 0;
}

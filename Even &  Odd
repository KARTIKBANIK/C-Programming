//Even , Odd program in C.

#include<stdio.h>

int main()

{
    int T,i,n;
    printf("Enter You Want: ");
    scanf("%d",&T);

    for (i=1; i<=T; i++)
    {
        printf("Enter your number: ");
        scanf("%d", &n);

        if (n%2==0)
        {
            printf("This is an even number.\n");

        }
        else
        {
            printf("This is odd number.\n" );
        }
    }
    return 0;
}
